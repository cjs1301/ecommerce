enum Gender {
  	male
  	female
}
enum OrderStatus {
	beforePayment // 미결제
	cancelled // 배송전 취소 신청
	overPaid // 결제 금액 초과
	overRefunded // 환불 금액 초과
	paid // 결제 완료
	partiallyRefunded // 부분 반품 완료
	placed // 배송완료
	refunded // 반품 완료
	underPaid // 결제 금액 미달
	underRefunded // 환불 금액 미달
	done // 주문 완료
}
enum ExchangeStatus {
	application // 교환 신청
	approval // 교환 승인
	cancelled // 교환 취소
	return // 교환 신청 반려
}
enum RefundStatus {
	accepted // 반품 승인
	cancelled // 반품 반려
	requested // 반품 요청
}
enum ProductType {
  	normal
	personalized
	bundle
}
enum DiscountType {
	percentage
	fixed
}
enum Role {
	master
	manager
}
enum FulfillmentStatus {
	pending // 배송 대기
	shipped // 배송 출고
	arrived // 배송 도착
}

